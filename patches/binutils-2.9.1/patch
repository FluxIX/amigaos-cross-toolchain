+		    fix_new(fragP, fragP->fr_fix, 2, 
+			    fragP->fr_symbol, fragP->fr_offset, 1, NO_RELOC, 0);
+	  	    fragP->fr_fix+=2;
+		  }
+                else if (flag_short_refs)
+                  {
